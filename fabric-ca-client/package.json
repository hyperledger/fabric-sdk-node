    "hyperledger",
    "blockchain"
  ],
  "version": "2.0.0-snapshot",
  "tag": "unstable",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --recursive  -t 10000"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/hyperledger/fabric-sdk-node"
  },
  "engines": {
    "node": "^8.9.0 || ^10.15.3 || ^12.13.1",
    "npm": "^5.5.1 || ^6.4.1"
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "fabric-common": "file:../fabric-common",
    "jsrsasign": "^7.2.2",
    "url": "^0.11.0",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "rewire": "^4.0.1",
    "sinon": "^6.1.3"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
