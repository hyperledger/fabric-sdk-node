{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@tsconfig/node10/tsconfig.json",
	"compilerOptions": {
		"declaration": true,
        "declarationMap": true,
		"sourceMap": true,
		"strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "lib",
        "rootDir": "src"
	}
}
